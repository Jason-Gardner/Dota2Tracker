@{
    ViewData["Title"] = "Home Page";
}

@model List<string>

<div class="text-center">
    @{ 
        List<string> thisMatch = new List<string>();
        thisMatch = (Model[0]).Split(',').ToList<string>();
    }
    @{ 
        foreach (string data in thisMatch)
        {
            <p>@data</p>
        }
    }
</div>
