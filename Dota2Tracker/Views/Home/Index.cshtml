@{
    ViewData["Title"] = "Home Page";
}

@model List<playerMatch>

<div class="text-center">
    <table>
        <thead>
            <tr>
                <th scope="col">Match ID</th>
                <th scope="col">Hero ID</th>
                <th scope="col">KDA</th>
            </tr>
        </thead>

        @for (int i = 0; i < 1; i++)
        {
            <tr>
                <td>@Model[i].matchID</td>
                <td>@Model[i].heroID</td>
                <td>@Model[i].matchKDA</td>
            </tr>
        }
    </table>
</div>
